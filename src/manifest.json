{
  "manifest_version": 3,
  "name": "タブポップアップ",
  "version": "1.0",
  "description": "開いているタブを別ウィンドウとしてポップアップします",
  "permissions": ["activeTab", "tabs", "windows"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
